BEGIN
  ss.clk = clk;
  TABLE
    i[4..0], ss => ss, o[7..0];
    B"11XXX", 1 => 3, B"10111000";
    B"00XXX", 1 => 2, B"11000000";
    B"10XXX", 1 => 4, B"00101000";
    B"0X0XX", 2 => 2, B"11000000";
    B"XX1XX", 2 => 5, B"00001110";
    B"110XX", 2 => 3, B"10111000";
    B"100XX", 2 => 4, B"00101000";
    B"10XXX", 3 => 4, B"00111000";
    B"00XXX", 3 => 2, B"11010000";
    B"11XXX", 3 => 3, B"10111000";
    B"01XXX", 3 => 6, B"00110101";
    B"010XX", 4 => 6, B"00100101";
    B"XX1XX", 4 => 7, B"00101000";
    B"110XX", 4 => 3, B"10111000";
    B"000XX", 4 => 2, B"11000000";
    B"100XX", 4 => 4, B"00101000";
    B"1X10X", 5 => 8, B"10000100";
    B"XX0XX", 5 => 2, B"11000000";
    B"XX11X", 5 => 8, B"10000100";
    B"0X10X", 5 => 5, B"00001110";
    B"XXXX1", 6 => 2, B"11000001";
