    B"11XX0", 6 => 3, B"10111001";
    B"01XX0", 6 => 6, B"00100101";
    B"XX0XX", 7 => 2, B"11000000";
    B"101XX", 7 => 7, B"00101000";
    B"011XX", 7 => 6, B"00100101";
    B"111XX", 7 => 3, B"10111000";
    B"001XX", 7 => 2, B"11000000";
    B"101XX", 8 => 7, B"00101000";
    B"XX0XX", 8 => 2, B"11000000";
    B"0X1XX", 8 => 8, B"10000100";
    B"111XX", 8 => 3, B"10111000";
  END TABLE;
END;
